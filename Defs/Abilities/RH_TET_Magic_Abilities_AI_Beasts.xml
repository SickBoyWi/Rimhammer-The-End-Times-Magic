													<tags>
														<li>AmberSpear</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Wildform</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
