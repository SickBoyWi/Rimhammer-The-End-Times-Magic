	<SickAbilityUserAI.AbilityUserAIProfileDef  Name="RH_TET_Magic_GreatMawWizardAI" Abstract="True">
		<compAbilityUserClass>TheEndTimes_Magic.CompMagicUser</compAbilityUserClass>
		<decisionTree>
			<subNodes>
				<!-- Not in combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<invert>true</invert>
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_CasterHealth">
							<maxHealth>.7</maxHealth>
							<subNodes>
								<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
									<tags>
										<li>Trollguts</li>
									</tags>
								</li>
							</subNodes>
						</li>
						<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
							<tags>
								<li>Toothcracker</li>
								<li>Bullgorger</li>
							</tags>
						</li>
					</subNodes>
				</li>
				<!-- In combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_HasEnemyTarget">
							<subNodes>
								<!-- Using ranged weapon -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingRangedWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>28.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>BoneCrusher</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Bullgorger</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>18.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Braingobbler</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Toothcracker</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Bullgorger</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
								<!-- Using melee weapon, agressive ability use. -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingMeleeWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>18.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Braingobbler</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Toothcracker</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Bullgorger</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>28.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>BoneCrusher</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Bullgorger</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
							</subNodes>
						</li>
					</subNodes>
				</li>
