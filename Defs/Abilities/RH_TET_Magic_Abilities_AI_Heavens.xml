	<SickAbilityUserAI.AbilityUserAIProfileDef  Name="RH_TET_Magic_HeavensWizardAI" Abstract="True">
		<compAbilityUserClass>TheEndTimes_Magic.CompMagicUser</compAbilityUserClass>
		<decisionTree>
			<subNodes>
				<!-- Not in combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<invert>true</invert>
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
							<tags>
								<li>HarmonicConvergence</li>
							</tags>
						</li>
					</subNodes>
				</li>
				<!-- In combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_HasEnemyTarget">
							<subNodes>
								<li Class="TheEndTimes_Magic.AbilityDecisionConditionalNode_EnemyTargetIsAnimal">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>24.0</maxDistance>
											<minDistance>10.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>AzureBlades</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
								<li Class="TheEndTimes_Magic.AbilityDecisionConditionalNode_EnemyTargetIsBuilding">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>45.0</maxDistance>
											<minDistance>20.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Comet</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
								<!-- Using ranged weapon -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingRangedWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>45.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Comet</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>35.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Thunderbolt</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Comet</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>HarmonicConvergence</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>20.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>AzureBlades</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Thunderbolt</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>HarmonicConvergence</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
								<!-- Using melee weapon, agressive ability use. -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingMeleeWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>20.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>AzureBlades</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>HarmonicConvergence</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Thunderbolt</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>35.0</maxDistance>
											<minDistance>10.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Thunderbolt</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>HarmonicConvergence</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>45.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>Comet</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
							</subNodes>
						</li>
					</subNodes>
				</li>
			</subNodes>
		</decisionTree>
	</SickAbilityUserAI.AbilityUserAIProfileDef>
	
