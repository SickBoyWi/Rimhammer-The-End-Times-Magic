	<SickAbilityUserAI.SickAbilityUserAIProfileDef Name="RH_TET_Magic_StealthWizardAI" Abstract="True">
		<compAbilityUserClass>TheEndTimes_Magic.CompMagicUser</compAbilityUserClass>
		<decisionTree>
			<subNodes>
				<!-- Not in combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<invert>true</invert>
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
							<tags>
								<li>SwiftScamper</li>
								<li>ArmorDarkness</li>
							</tags>
						</li>
					</subNodes>
				</li>
				<!-- In combat -->
				<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_InCombat">
					<subNodes>
						<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_HasEnemyTarget">
							<subNodes>
								<!-- Using ranged weapon -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingRangedWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>28.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ArmorDarkness</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>SwiftScamper</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ShadowsCompanion</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>18.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ShadowsCompanion</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ArmorDarkness</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>SwiftScamper</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
								<!-- Using melee weapon, agressive ability use. -->
								<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_UsingMeleeWeapon">
									<subNodes>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>18.0</maxDistance>
											<minDistance>5.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ShadowsCompanion</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ArmorDarkness</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>SwiftScamper</li>
													</tags>
												</li>
											</subNodes>
										</li>
										<li Class="SickAbilityUserAI.AbilityDecisionConditionalNode_EnemyTargetDistance">
											<maxDistance>28.0</maxDistance>
											<minDistance>15.0</minDistance>
											<subNodes>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ArmorDarkness</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>SwiftScamper</li>
													</tags>
												</li>
												<li Class="SickAbilityUserAI.AbilityDecisionNode_PickAbilityWithMatchingTags">
													<tags>
														<li>ShadowsCompanion</li>
													</tags>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
							</subNodes>
						</li>
					</subNodes>
				</li>
			</subNodes>
		</decisionTree>
	</SickAbilityUserAI.SickAbilityUserAIProfileDef>
	
