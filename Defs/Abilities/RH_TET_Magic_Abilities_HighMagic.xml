	<!-- ===================== Flames of the Phoenix =============================== -->
  
	<ThingDef ParentName="BaseBullet" Class="AbilityUser.ProjectileDef_Ability">
		<thingClass>AbilityUser.Projectile_AbilityBase</thingClass>
		<defName>RH_TET_HighPhoenixFlamesP</defName>
		<label>flames of the phoenix</label>
		<graphicData>
			<texPath>Things/Projectile/High/RH_TET_HighPhoenixFlames</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
		</graphicData>
		<projectile>
			<flyOverhead>false</flyOverhead>
			<damageDef>RH_TET_MagicFire</damageDef>
			<damageAmountBase>20</damageAmountBase>
			<speed>80</speed>
		</projectile>
	</ThingDef>

	<TheEndTimes_Magic.MagicAbilityDef ParentName="RH_TET_BaseMagicAbility">
		<defName>RH_TET_HighPhoenixFlames_Mage</defName>
		<label>Flames of the Phoenix I</label>
		<uiIconPath>UI/Magic/RH_TET_HighPhoenixFlames</uiIconPath>
		<description>Pure white flames emerge from the air itself and envelop the target, immolating the unworthy foe. With every passing second the flames grow hotter, swiftly intensifying until they can sear flesh or even melt steel.</description>
		<magicPoolCost>0.30</magicPoolCost>
		<abilityPoints>1</abilityPoints>
		<experiencePoints>17</experiencePoints>
		<nextMagicLevelPointsRequired>1</nextMagicLevelPointsRequired><!-- points to acquire this spell -->
		<MainVerb Class="AbilityUser.VerbProperties_Ability">
			<verbClass>TheEndTimes_Magic.Verb_PhoenixFlames</verbClass>
			<tooltipShowProjectileDamage>true</tooltipShowProjectileDamage>
			<tooltipShowExtraDamages>true</tooltipShowExtraDamages>
			<tooltipShowHediffsToApply>false</tooltipShowHediffsToApply>
			<tooltipShowMentalStatesToApply>false</tooltipShowMentalStatesToApply>
			<hasStandardCommand>true</hasStandardCommand>
			<requiresLineOfSight>false</requiresLineOfSight>
			<defaultProjectile>RH_TET_HighPhoenixFlamesP</defaultProjectile>
			<abilityDef>RH_TET_HighPhoenixFlames_Mage</abilityDef>
			<warmupTime>3</warmupTime>
			<SecondsToRecharge>240</SecondsToRecharge>
			<range>30</range>
			<burstShotCount>1</burstShotCount>
			<soundCast>RH_TET_Magic_SoundWhoosh</soundCast>
			<soundCastTail>GunTail_Medium</soundCastTail>
			<muzzleFlashScale>5</muzzleFlashScale>
			<AlwaysHits>true</AlwaysHits>
			<isPrimary>true</isPrimary>
			<isViolent>true</isViolent>
			<AbilityTargetCategory>TargetAoE</AbilityTargetCategory>
			<TargetAoEProperties>
				<targetClass>Pawn</targetClass>
				<showRangeOnSelect>true</showRangeOnSelect>
				<friendlyFire>false</friendlyFire>
				<startsFromCaster>false</startsFromCaster>
				<maxTargets>2</maxTargets>
				<range>3</range>
			</TargetAoEProperties>
			<targetParams>
				<canTargetPawns>true</canTargetPawns>
				<canTargetBuildings>false</canTargetBuildings>
				<canTargetSelf>false</canTargetSelf>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
			<extraDamages>
				<li>
					<damage>10</damage>
					<damageDef>RH_TET_MagicFire</damageDef>
					<chance>1</chance>
				</li>
			</extraDamages>
		</MainVerb>
	</TheEndTimes_Magic.MagicAbilityDef>
	
