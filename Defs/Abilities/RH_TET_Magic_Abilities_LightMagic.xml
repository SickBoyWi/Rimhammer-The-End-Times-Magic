			<SecondsToRecharge>120</SecondsToRecharge>
			<range>20</range>
			<burstShotCount>1</burstShotCount>
			<soundCast>RH_TET_Magic_SoundChains</soundCast>
			<muzzleFlashScale>5</muzzleFlashScale>
			<AlwaysHits>true</AlwaysHits>
