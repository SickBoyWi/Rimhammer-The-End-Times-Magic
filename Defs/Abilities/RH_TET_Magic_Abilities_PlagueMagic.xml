	<!-- ===================== Great Red Pox =============================== -->
  
	<ThingDef ParentName="BaseBullet" Class="SickAbilityUser.ProjectileDef_Ability">
		<thingClass>SickAbilityUser.Projectile_AbilityBase</thingClass>
		<defName>RH_TET_PlagueRedPoxP</defName>
		<label>Great Red Pox</label>
		<graphicData>
			<texPath>UI/Magic/RH_TET_PlagueRedPox</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
		</graphicData>
		<projectile>
			<flyOverhead>true</flyOverhead>
			<damageDef>RH_TET_MagicalAugmentation</damageDef>
			<damageAmountBase>0</damageAmountBase>
			<speed>999</speed>
			<explosionRadius>5</explosionRadius>
			<postExplosionSpawnChance>1</postExplosionSpawnChance>
		</projectile>
	</ThingDef>
	
	<TheEndTimes_Magic.MagicAbilityDef ParentName="RH_TET_BaseMagicAbility">
		<defName>RH_TET_PlagueRedPox_Mage</defName>
		<label>Great Red Pox I</label>
		<uiIconPath>UI/Magic/RH_TET_PlagueRedPox</uiIconPath>
		<description>The caster unleashes a crimson mist upon enemies near or far away on the battlefield, causing an instantaneous eruption of debilitating fist-sized boils. The target is slowed, mentally and physically, and is subjected to much pain.</description>
		<magicPoolCost>0.20</magicPoolCost>
		<abilityPoints>1</abilityPoints>
		<experiencePoints>13</experiencePoints>
		<nextMagicLevelPointsRequired>1</nextMagicLevelPointsRequired><!-- points to acquire this spell -->
		<MainVerb Class="SickAbilityUser.VerbProperties_Ability">
			<verbClass>TheEndTimes_Magic.Verb_RedPox</verbClass>
			<tooltipShowProjectileDamage>true</tooltipShowProjectileDamage>
			<tooltipShowExtraDamages>false</tooltipShowExtraDamages>
			<tooltipShowHediffsToApply>false</tooltipShowHediffsToApply>
			<tooltipShowMentalStatesToApply>false</tooltipShowMentalStatesToApply>
			<hasStandardCommand>true</hasStandardCommand>
			<requiresLineOfSight>false</requiresLineOfSight>
			<defaultProjectile>RH_TET_PlagueRedPoxP</defaultProjectile>
			<abilityDef>RH_TET_PlagueRedPox_Mage</abilityDef>
			<warmupTime>2</warmupTime>
			<SecondsToRecharge>120</SecondsToRecharge>
			<range>40</range>
			<burstShotCount>1</burstShotCount>
			<soundCast>Message_NegativeHealthEvent</soundCast>
			<soundCastTail>GunTail_Medium</soundCastTail>
			<muzzleFlashScale>5</muzzleFlashScale>
			<AlwaysHits>true</AlwaysHits>
			<isPrimary>true</isPrimary>
			<isViolent>false</isViolent>
			<AbilityTargetCategory>TargetAoE</AbilityTargetCategory>
			<TargetAoEProperties>
				<targetClass>Pawn</targetClass>
				<showRangeOnSelect>false</showRangeOnSelect>
				<friendlyFire>true</friendlyFire>
				<startsFromCaster>false</startsFromCaster>
				<maxTargets>2</maxTargets>
				<range>3</range>
			</TargetAoEProperties>
			<hediffsToApply>
				<li>
					<hediffDef>RH_TET_PlagueRedPoxI</hediffDef>
					<applyChance>.8</applyChance>
				</li>
			</hediffsToApply>
		</MainVerb>
	</TheEndTimes_Magic.MagicAbilityDef>
	
