	<ThingDef ParentName="BaseBullet" Class="AbilityUser.ProjectileDef_AbilityLaser">
		<thingClass>AbilityUser.Projectile_AbilityLaser</thingClass>
		<defName>RH_TET_WarpLightningP</defName>
		<label>warp lightning</label>
		<graphicData>
			<texPath>Things/Projectile/Tzeentch/RH_TET_GreenBoltOfTzeentch</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile>
			<flyOverhead>false</flyOverhead>
			<damageDef>RH_TET_Lightning</damageDef>
			<damageAmountBase>32</damageAmountBase>
			<speed>999</speed>
		</projectile>
		<preFiringInitialIntensity>0</preFiringInitialIntensity>
		<preFiringFinalIntensity>20</preFiringFinalIntensity>
		<postFiringInitialIntensity>20</postFiringInitialIntensity>
		<postFiringFinalIntensity>0</postFiringFinalIntensity>
		<preFiringDuration>12</preFiringDuration>
		<postFiringDuration>75</postFiringDuration>
		<StartFireChance>0.100</StartFireChance>
	</ThingDef>

	<TheEndTimes_Magic.MagicAbilityDef ParentName="RH_TET_BaseMagicAbility">
		<defName>RH_TET_WarpLightning_Mage</defName>
		<label>Warp Lightning I</label>
		<uiIconPath>UI/Magic/RH_TET_WarpLightning</uiIconPath>
		<description>Lash out with warp lightning, and zzzzap a foe.</description>
		<magicPoolCost>0.22</magicPoolCost>
		<abilityPoints>1</abilityPoints>
		<experiencePoints>13</experiencePoints>
		<nextMagicLevelPointsRequired>1</nextMagicLevelPointsRequired><!-- points to acquire this spell. -->
		<MainVerb Class="AbilityUser.VerbProperties_Ability">
			<verbClass>TheEndTimes_Magic.Verb_UseAbility_TrueBurstMiscast</verbClass>
			<tooltipShowProjectileDamage>true</tooltipShowProjectileDamage>
			<tooltipShowExtraDamages>false</tooltipShowExtraDamages>
			<tooltipShowHediffsToApply>false</tooltipShowHediffsToApply>
			<tooltipShowMentalStatesToApply>false</tooltipShowMentalStatesToApply>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>RH_TET_WarpLightningP</defaultProjectile>
			<abilityDef>RH_TET_WarpLightning_Mage</abilityDef>
			<warmupTime>1</warmupTime>
			<SecondsToRecharge>20</SecondsToRecharge>
			<range>34</range>
			<isViolent>true</isViolent>
			<burstShotCount>1</burstShotCount>
			<ticksBetweenBurstShots>2</ticksBetweenBurstShots>
			<soundCast>RH_TET_ElectricitySound</soundCast>
			<muzzleFlashScale>20</muzzleFlashScale>
			<isPrimary>false</isPrimary>
			<canCastInMelee>true</canCastInMelee>
		</MainVerb>
	</TheEndTimes_Magic.MagicAbilityDef>

